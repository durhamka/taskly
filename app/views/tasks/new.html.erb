<div class="task-form">
  <h1>Add a task list</h1>
  <%= form_for [@task_list, @task] do |f| %>
    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :due_date %>
    <%= f.date_select :due_date %>

    <div id="submit">
      <br><%= f.submit "Create Task" %>
    </div>
  <% end %>
</div>